{
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "plugins": [ "react" ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
      "browser": true,
      "node": true,
      "jasmine": true
  },
  "rules": {
    "semi": 0
  }
}
